public class ContinueInDoWhileLoop {
    public static void main(String[] args) {
        int i = 1;

        do {
            // Skip the iteration when i is even
            if (i % 2 == 0) {
                i++;
                continue;
            }

            System.out.println("Iteration: " + i);
            i++;

        } while (i <= 5);
    }
}
